<h1>Listing analogues</h1>

<table>
  <tr>
    <th>Original</th>
    <th>Analogue</th>
    <th>Note</th>
  </tr>

<% @analogues.each do |analogue| %>
  <tr>
    <td><%=h analogue.original_id %></td>
    <td><%=h analogue.analogue_id %></td>
    <td><%=h analogue.note %></td>
    <td><%= link_to 'Show', analogue %></td>
    <td><%= link_to 'Edit', edit_analogue_path(analogue) %></td>
    <td><%= link_to 'Destroy', analogue, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New analogue', new_analogue_path %>