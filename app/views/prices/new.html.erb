<h1>New price</h1>

<% form_for(@price) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :code %><br />
    <%= f.text_field :code %>
  </p>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :contractor %><br />
    <%= f.text_field :contractor %>
  </p>
  <p>
    <%= f.label :margin %><br />
    <%= f.text_field :margin %>
  </p>
  <p>
    <%= f.label :original %><br />
    <%= f.check_box :original %>
  </p>
  <p>
    <%= f.label :file_mask %><br />
    <%= f.text_field :file_mask %>
  </p>
  <p>
    <%= f.label :settings_id %><br />
    <%= f.text_field :receive_settings_id %>
  </p>
  <p>
    <%= f.label :settings_type %><br />
    <%= f.text_field :receive_settings_type %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', prices_path %>