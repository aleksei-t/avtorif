<h1>Listing goods_auto_options</h1>

<table>
  <tr>
    <th>Goods</th>
    <th>Auto option</th>
  </tr>

<% @goods_auto_options.each do |goods_auto_option| %>
  <tr>
    <td><%=h goods_auto_option.goods %></td>
    <td><%=h goods_auto_option.auto_option %></td>
    <td><%= link_to 'Show', goods_auto_option %></td>
    <td><%= link_to 'Edit', edit_goods_auto_option_path(goods_auto_option) %></td>
    <td><%= link_to 'Destroy', goods_auto_option, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New goods_auto_option', new_goods_auto_option_path %>