<h1>Editing import</h1>

<% form_for(@import) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :price %><br />
    <%= f.text_field :price %>
  </p>
  <p>
    <%= f.label :importable %><br />
    <%= f.text_field :importable %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @import %> |
<%= link_to 'Back', imports_path %>