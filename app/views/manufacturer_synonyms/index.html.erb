<h1>Listing manufacturer_synonyms</h1>

<table>
  <tr>
    <th>Manufacturer</th>
    <th>Title</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @manufacturer_synonyms.each do |manufacturer_synonym| %>
  <tr>
    <td><%= manufacturer_synonym.manufacturer %></td>
    <td><%= manufacturer_synonym.title %></td>
    <td><%= link_to 'Show', manufacturer_manufacturer_synonym_path(manufacturer_synonym.manufacturer_id, manufacturer_synonym) %></td>
    <td><%= link_to 'Edit', edit_manufacturer_manufacturer_synonym_path(manufacturer_synonym.manufacturer_id, manufacturer_synonym) %></td>
    <td><%= link_to 'Destroy', manufacturer_manufacturer_synonym_path(manufacturer_synonym.manufacturer_id, manufacturer_synonym), :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

