<h1>Listing column_filters</h1>

<table>
  <tr>
    <th>First</th>
    <th>Second</th>
    <th>Third</th>
    <th>Fourth</th>
    <th>Fifth</th>
    <th>Sixth</th>
    <th>Seventh</th>
    <th>Eighth</th>
    <th>Ninth</th>
    <th>Tenth</th>
  </tr>

<% @column_filters.each do |column_filter| %>
  <tr>
    <td><%=h column_filter.first %></td>
    <td><%=h column_filter.second %></td>
    <td><%=h column_filter.third %></td>
    <td><%=h column_filter.fourth %></td>
    <td><%=h column_filter.fifth %></td>
    <td><%=h column_filter.sixth %></td>
    <td><%=h column_filter.seventh %></td>
    <td><%=h column_filter.eighth %></td>
    <td><%=h column_filter.ninth %></td>
    <td><%=h column_filter.tenth %></td>
    <td><%= link_to 'Show', column_filter %></td>
    <td><%= link_to 'Edit', edit_column_filter_path(column_filter) %></td>
    <td><%= link_to 'Destroy', column_filter, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New column_filter', new_column_filter_path %>